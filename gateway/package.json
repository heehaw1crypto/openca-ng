{
  "name": "gateway",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.ts",
  "scripts": {
    "test": "source ./.local && mocha test/all/*.js",
    "minitest": "mocha test/dev/*.js",
    "pm2": "source ./.local && pm2 start ./pm2.config.json",
    "pm2:deploy": "pm2 start ./pm2.deploy.config.json",
    "load-data": "source ./.local && node ./initialization",
    "load-data-bash": "bash .local && node ./initialization",
    "lint": "eslint --ext .js --ext .ts .",
    "compile": "tsc",
    "setup": "source ./.local && mocha test/setup/init.js",
    "regen-sdk": "source .local && node ./src/services/db-service/updateService.js $DB_USER $DB_PASSWORD"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/multer": "^1.4.2",
    "@wesleytodd/openapi": "^0.0.8",
    "ajv": "^6.10.2",
    "bluebird": "3.5.5",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "expo-server-sdk": "^3.5.1",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "expression-eval": "^3.1.0",
    "globby": "11.0.0",
    "http-errors": "^1.7.3",
    "json-schema-traverse": "0.4.1",
    "jwt-simple": "^0.5.6",
    "lodash": "^4.17.20",
    "merge-deep": "^3.0.2",
    "moment": "^2.24.0",
    "mongo-sql": "4.0.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "mysql": "^2.18.1",
    "node-fetch": "^2.6.0",
    "passport": "^0.4.1",
    "passport-google-oauth2": "^0.2.0",
    "passport-oauth2": "^1.5.0",
    "path-to-regexp": "^2.4.0",
    "postmark": "^2.5.5",
    "qrcode": "^1.4.4",
    "qs": "^6.9.3",
    "redoc": "^2.0.0-alpha.41",
    "router": "^1.3.3",
    "serve-static": "^1.13.2",
    "slack-notify": "^0.1.7",
    "socket.io": "^2.3.0",
    "swagger-parser": "8.0.4",
    "swagger-ui-dist": "^3.20.2",
    "tookan-api": "^1.0.4",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.9.0",
    "@typescript-eslint/parser": "^2.9.0",
    "camelcase": "^6.2.0",
    "chai": "^4.2.0",
    "eslint": "6.7.1",
    "eslint-plugin-microservice-sdk": "https://bitbucket.org/apalm/eslint-plugin-microservice-sdk.git#27e4d24bc3922ac6d52496e4108f9a40a8574ee0",
    "form-data": "^3.0.0",
    "mustache": "^4.1.0",
    "nodemon": "1.18.9",
    "socket.io-client": "^2.3.0",
    "to-slug-case": "^1.0.0",
    "ts-node": "7.0.1",
    "typescript": "^3.8.3"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended"
    ],
    "env": {
      "es6": true,
      "node": true
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "project": "./tsconfig.json"
    },
    "plugins": [
      "@typescript-eslint",
      "microservice-sdk"
    ],
    "rules": {
      "require-atomic-updates": "off",
      "no-useless-catch": "off",
      "no-unused-vars": "off",
      "microservice-sdk/no-missing-valid": "error"
    }
  }
}
